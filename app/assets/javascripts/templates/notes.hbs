<div class="row">
  <h2>My Notes</h2>
  <div class="col-xs-4">
  Total Notes: {{noteCount}}
    <ul class="nav nav-pills nav-stacked">
      {{#each}}
        {{#link-to 'note' this tagName='li'}}
          <a href='#'>{{title}}</a>
        {{/link-to}}
      {{/each}}
    </ul>
  </div>
  <div class="col-xs-8">
    {{outlet}}
  </div>
</div>

{{render 'recent' mostRecent tagName='footer'}}
